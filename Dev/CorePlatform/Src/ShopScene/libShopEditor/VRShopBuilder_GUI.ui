/*	Push button*/
QPushButton {
	min-height: 20px;
	border: 1px solid gray;
	border-radius: 3px;
	background-color: lightgray;
	selection-background-color: gray;
}

QPushButton:hover {
	background-color: gray;
	border-radius: 3px;
}

/*Menu bar*/
QMenuBar {
	background-color:rgb(105,105,105);
	spacing: 0px;
}

QMenuBar::item {
	background-color: transparent;
}

QMenu {
	background-color:rgb(105,105,105);
	border: 1px solid black;
}

/*Combo box*/
QComboBox {
	min-height: 19px;
	background-color: lightgray;
	selection-background-color: gray;
	padding-left: 4px;
	border: 1px solid gray;
}

QComboBox::down-arrow {
	image: url(:/arrow_down.png);
    width: 10px;
    height: 10px;
}

QComboBox::drop-down {
	border: 0px;
    subcontrol-position: top right;
    width: 15px;
}

QComboBox QAbstractItemView {
	border: 1px solid black;
	background-color: lightgray;
	selection-background-color: gray;
}


QMenuBar::item:selected, QMenu::item:selected,QListWidget::item:selected, QComboBox:hover
{
	background-color: gray;
}


/*Tool button*/
QToolButton {
	max-width: 20px;
	max-height: 20px;
	border: 0px solid gray;
}

/*Line edit*/
QLineEdit {
    border: 1px solid gray;
	border-radius: 3px;
    background-color: lightgray;
    selection-background-color: darkgray;
}

/*Double spin box
QDoubleSpinBox {
    border: 1px solid gray;
	border-radius: 3px;
    background-color: lightgray;
    selection-background-color: darkgray;
    padding-right: 15px;
	min-height: 19px;
	max-height: 19px;
}

*/
QSpinBox {
	border: 1px solid gray;
	border-radius: 3px;
}

QSpinBox::up-button, QSpinBox::down-button {
	width: 70px;
	height: 70px;
	border: 1px solid gray;
	border-radius: 3px; 
/*	background-color: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop: 0 #ffe670, stop: 1 #ffde40);
*/
}

QSpinBox::up-arrow, QSpinBox::down-arrow {
/*	border-left: 17px solid none;
	border-right: 17px solid none;
	border-bottom: 17px solid black;
	width: 0px;
	height: 0px;
*/
}

QSpinBox::up-button:off, QSpinBox::down-button:off {
	background-color: lightgray;
	color: #eeeeee;
}

QSpinBox::up-arrow:off {border-left: 17px solid none; border-right: 17px solid none; border-bottom: 17px solid #eeeeee; background-color:lightgrey}

QSpinBox::down-arrow:off {border-left: 17px solid none; border-right: 17px solid none; border-top: 17px solid #eeeeee; background-color: lightgrey}</string>
